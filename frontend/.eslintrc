{
    "env": {
        "browser": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "globals": {
        "React": false
    },
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true,
            "experimentalObjectRestSpread": true
        }
    },
    "rules": {
        "no-case-declarations": 0,
        "no-cond-assign": 0,
        "no-extra-semi": 0,
        "no-unused-vars": 1,
        "react/display-name": 0,
        "react/prop-types": 0,
        "react/no-find-dom-node": 0
    }
}